<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<VBox alignment="TOP_CENTER" spacing="10.0" stylesheets="@../../../../../../styles/main.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="caillou.company.clonemanager.gui.customComponent.locationContainer.LocationContainerController">
    <children>
        <HBox prefHeight="100.0" prefWidth="200.0" spacing="50.0">
            <children>
                <VBox prefHeight="200.0" prefWidth="100.0" styleClass="boxDashed" stylesheets="@../../../../../../styles/main.css">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Label styleClass="boxMiddleTitle" text="%group1" />
                            </children>
                        </HBox>
                        <VBox fx:id="locationContainerGroupA" prefHeight="200.0" prefWidth="100.0" spacing="5.0" VBox.vgrow="NEVER">
                     <padding>
                        <Insets bottom="8.0" />
                     </padding></VBox>
                        <HBox>
                            <children>
                                <Hyperlink onAction="#addLocationGroupAFromLink" text="%button.add" />
                                <ImageView id="addLocation48x48.png" fitHeight="16.0" fitWidth="16.0" onMouseClicked="#addLocationGroupAFromButton">
                                    <image>
                                        <javafx.scene.image.Image preserveRatio="true" smooth="true" url="@/img/addLocation48x48Orange.png" />
                                    </image>
                                </ImageView>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0" styleClass="boxDashed" stylesheets="@../../../../../../styles/main.css">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Label styleClass="boxMiddleTitle" text="%group2" />
                            </children>
                        </HBox>
                        <VBox fx:id="locationContainerGroupB" prefHeight="200.0" prefWidth="100.0" spacing="5.0" VBox.vgrow="NEVER">
                     <padding>
                        <Insets bottom="8.0" />
                     </padding></VBox>
                        <HBox>
                            <children>
                                <Hyperlink onAction="#addLocationGroupBFromLink" text="%button.add" />
                                <ImageView id="addLocation48x48.png" fitHeight="16.0" fitWidth="16.0" onMouseClicked="#addLocationGroupBFromButton">
                                    <image>
                                        <javafx.scene.image.Image preserveRatio="true" smooth="true" url="@/img/addLocation48x48Orange.png" />
                                    </image>
                                </ImageView>
                            </children>
                        </HBox>    
                    </children>
                </VBox>
            </children>
            <padding>
                <Insets left="50.0" />
            </padding>
        </HBox>
        <VBox fx:id="locationContainerId" spacing="5.0">
            <padding>
                <Insets left="5.0" />
            </padding>
        </VBox>
        <VBox spacing="5.0">
            <children>
                <CheckBox fx:id="detectDoublonsWithinSameLocationId" mnemonicParsing="false" text="%locationContainer.detectDuplicateWithinALocation">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </CheckBox>
            </children>
            <padding>
                <Insets left="5.0" />
            </padding>
        </VBox>
    </children>
</VBox>
